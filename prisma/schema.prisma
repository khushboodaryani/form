generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  // url was removed because Prisma v7 expects connection config in prisma.config.ts
}

model Enquiry {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  company     String
  gender      String
  age         Int
  email       String
  contact     String
  query       String
  disposition String
  createdAt   DateTime @default(now())
}
