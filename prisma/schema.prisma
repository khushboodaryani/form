generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Enquiry {
  id          Int      @id @default(autoincrement())
  name        String
  company     String
  gender      String
  age         Int
  email       String
  contact     String
  query       String
  disposition String
  createdAt   DateTime @default(now())
}
