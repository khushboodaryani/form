generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Enquiry {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  company     String
  gender      String
  age         Int
  email       String
  contact     String
  query       String
  disposition String
  createdAt   DateTime @default(now())
}
